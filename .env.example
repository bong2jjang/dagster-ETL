# =============================================================================
# Dagster ETL 환경 설정 예시
# 이 파일을 .env로 복사하여 실제 값을 입력하세요
# =============================================================================

# Dagster 환경
DAGSTER_ENVIRONMENT=dev
DAGSTER_HOME=.

# =============================================================================
# Trino
# =============================================================================
TRINO_HOST=localhost
TRINO_PORT=18080
TRINO_USER=admin
TRINO_HTTP_SCHEME=http
TRINO_CATALOG=iceberg

# =============================================================================
# S3 / MinIO
# =============================================================================
S3_ENDPOINT_URL=http://localhost:9000
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
AWS_REGION=us-east-1
S3_BUCKET_NAME=data-lake

# =============================================================================
# Project 01 테넌트 - Source DB
# =============================================================================
PROJECT_01_DB_HOST=localhost
PROJECT_01_DB_PORT=5432
PROJECT_01_DB_NAME=your_db_name
PROJECT_01_DB_USER=your_user
PROJECT_01_DB_PASSWORD=your_password

# =============================================================================
# Default 테넌트 - Source DB
# =============================================================================
DEFAULT_DB_HOST=localhost
DEFAULT_DB_PORT=5432
DEFAULT_DB_NAME=your_db_name
DEFAULT_DB_USER=your_user
DEFAULT_DB_PASSWORD=your_password
